  <xsd:annotation>
    <xsd:documentation>
      This XML Schema expose the service types(Convience types to use with the web service binding) of the MSN AddressBook Web service
      used by Windows Live Messenger. Microsoft did not disclose the description of
      the web service so we are stuck reverse engineering it. This XML Schema along with the WSDL
      description will automate the generation of a WDSL binding to MSN AddressBook Web service.
    </xsd:documentation>
  </xsd:annotation>
  <xsd:element name="ABApplicationHeader">
    <xsd:annotation>
      <xsd:documentation>
        ABApplicationHeader is a part of request SOAP header, it identify the application information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element fixed="09607671-1C32-421F-A6A6-CBFAA51AB5F4" name="ApplicationId" type="msnab:Guid" />
        <xsd:element name="IsMigration" type="xsd:boolean" />
        <xsd:element name="PartnerScenario" type="xsd:string" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ABAuthHeader">
    <xsd:annotation>
      <xsd:documentation>
        ABAuthHeader meaning unknow for the moment but it's a part of request SOAP header.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="ManagedGroupRequest" type="xsd:boolean" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ServiceHeader">
    <xsd:annotation>
      <xsd:documentation>
        ServiceHeade is the SOAP Header received in a MSN AddressBook webservice response.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Version" type="xsd:string" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="FindMembershipRequestType">
    <xsd:annotation>
      <xsd:documentation>
        Parameters to the FindMembership SOAP Action
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="serviceFilter">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Types">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="ServiceType" type="xsd:string" />
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="View" type="xsd:string" />
      <xsd:element minOccurs="0" name="deltasOnly" type="xsd:boolean" />
      <xsd:element minOccurs="0" name="lastChange" type="xsd:dateTime" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="FindMembership" type="msnab:FindMembershipRequestType" />
  <xsd:complexType name="FindMembershipResultType">
    <xsd:annotation>
      <xsd:documentation>
        FindMembershipResult contains a list of Service and the "myself" information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Services">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Service" type="msnab:ServiceType" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="OwnerNamespace">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Info">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Handle">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="Id" type="xsd:string" />
                        <xsd:element name="IsPassportNameHidden" type="xsd:boolean" />
                        <xsd:element name="CID" type="xsd:integer" />
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="CreatorPuid" type="xsd:integer" />
                  <xsd:element name="CreatorCID" type="xsd:integer" />
                  <xsd:element name="CreatorPassportName" type="xsd:string" />
                  <xsd:element name="CircleAttributes">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="IsPresenceEnabled" type="xsd:boolean" />
                        <xsd:element name="IsEvent" type="xsd:boolean" />
                        <xsd:element name="Domain" type="xsd:string" />
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="Changes" type="xsd:string" />
            <xsd:element name="CreateDate" type="xsd:dateTime" />
            <xsd:element name="LastChange" type="xsd:dateTime" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="FindMembershipResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="FindMembershipResult" type="msnab:FindMembershipResultType" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="ABFindAllRequestType">
    <xsd:annotation>
      <xsd:documentation>
        Parameters to the ABFindAll SOAP Action
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element fixed="00000000-0000-0000-0000-000000000000" name="abId" type="msnab:Guid" />
      <xsd:element minOccurs="0" name="abView" type="xsd:string" />
      <xsd:element minOccurs="0" name="deltasOnly" type="xsd:boolean" />
      <xsd:element minOccurs="0" name="lastChange" type="xsd:dateTime" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="ABFindAll" type="msnab:ABFindAllRequestType" />
  <xsd:complexType name="ABFindAllResultType">
    <xsd:annotation>
      <xsd:documentation>
        ABFindAllResult contains a list of contacts and address
        book information.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="groups">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Group" type="msnab:GroupType" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="contacts">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Contact" type="msnab:ContactType" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CircleResult">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CircleTicket" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ab">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="abId" type="msnab:Guid" />
            <xsd:element name="abInfo">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="ownerPuid" type="xsd:integer" />
                  <xsd:element name="OwnerCID" type="xsd:integer" />
                  <xsd:element name="ownerEmail" type="xsd:string" />
                  <xsd:element name="fDefault" type="xsd:boolean" />
                  <xsd:element name="joinedNamespace" type="xsd:boolean" />
                  <xsd:element name="IsBot" type="xsd:boolean" />
                  <xsd:element name="IsParentManaged" type="xsd:boolean" />
                  <xsd:element name="SubscribeExternalPartner" type="xsd:boolean" />
                  <xsd:element name="NotifyExternalPartner" type="xsd:boolean" />
                  <xsd:element name="AddressBookType" type="xsd:string" />
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="lastChange" type="xsd:dateTime" />
            <xsd:element name="DynamicItemLastChanged" type="xsd:dateTime" />
            <xsd:element name="createDate" type="xsd:dateTime" />
            <xsd:element name="propertiesChanged" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="ABFindAllResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="ABFindAllResult" type="msnab:ABFindAllResultType" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
